{
  "name": "personal-js",
  "version": "1.0.0",
  "description": "Server for personal usage and codesharing purposes",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "export NODE_ENV=development; node -e \"require('./tools/jsBundler.js').watchScripts()\"",
    "build": "export NODE_ENV=production; node -e \"require('./tools/jsBundler.js').buildScripts()\"",
    "watch:styles": "export NODE_ENV=development; node -e \"require('./tools/lessBundler.js').watchLess()\"",
    "build:styles": "export NODE_ENV=production; node -e \"require('./tools/lessBundler.js').buildLess()\"",
    "refresh": "rm -rf build/ && mkdir build/ && mkdir build/css && mkdir build/js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pwroff/personal-js.git"
  },
  "author": "Leonid Lazaryev <leonidlazaryev@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pwroff/personal-js/issues"
  },
  "homepage": "https://github.com/pwroff/personal-js#readme",
  "dependencies": {
    "babel": "^6.5.2",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "envify": "^4.0.0",
    "express": "^4.14.0",
    "less": "^2.7.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "reactify": "^1.1.1",
    "uglify-js": "^2.7.5",
    "watchify": "^3.7.0"
  }
}
